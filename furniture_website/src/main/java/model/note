User(
	id String
	firstName String
	lastName String
	userName String
	password String
	email String
	createAt Date
	access String //dể quản lý quyền của các tài khoản, thay vì tạo nhiều tài khoản quản trị riêng biệt
	birthday Date
	gender Gender
	phoneNumber String
	address String  // địa chỉ người dùng
	deliveryAddress String // địa chỉ giao hàng
	profilePicture String  // đương dãn ảnh 
)

Products(
	id  String
	name String
	group Group // nhóm các sản phẩm
	categories Category // thể loại
	amount double
	price double
)

Orders(
	id  String
	orderDate  Date
	shippingCost double
	orderStatus String
	coupon Coupon
	listProduct arraylist<OrderDetails>
)

OrderDetails(
	product Product
	quality int
	totalAmount double
)

Cart(
	id
	user User
	shippingCost double 
	totalPrice double 
	
)

CartDetails(
	product Product
	price double
	quality int
	isGift boolean
	isAvalable boolean
)

